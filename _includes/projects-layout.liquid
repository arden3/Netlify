  <!-- Modal contact form -->
    <section id="my-modal" class="modal">
      <div class="modal-content">
        <div class="modal-header">
          <span class="modal-close"
            ><img src="/img/pages/modal-close.png" alt=""
          /></span>
          <h4>Contact Mondreem</h4>
          <p class="modal-subtext">
            <br />
            Drop us a note and we'll get back to you!
          </p>
        </div>
        <div class="modal-body">
          <form name="magazine" method="POST" data-netlify="true">
            <input
              type="text"
              name="name"
              id="first-name"
              class="modal-input"
              placeholder="Full name"
            />
            <input
              type="email"
              name="email"
              id="email-address"
              class="modal-input"
              placeholder="Email Address"
            />
            <textarea
              name="message"
              placeholder="Type your message here"
              class="modal-input-main"
            ></textarea>
            <button class="btn modal-btn" type="submit">Send</button>
          </form>
        </div>
      </div>
    </section>
  
